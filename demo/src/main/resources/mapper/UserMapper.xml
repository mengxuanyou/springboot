<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wbkit.bigscreen.mapper.UserMapper">

    <select id="getAllUser" resultType="com.wbkit.bigscreen.entity.User">
        SELECT * FROM DEMO
    </select>

    <insert id="insert" parameterType="com.wbkit.bigscreen.entity.User">
        INSERT INTO DEMO(id, name) VALUES (#{id},#{name})
    </insert>
</mapper>